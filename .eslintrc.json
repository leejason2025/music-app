{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "react-native/react-native": true
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "react/jsx-filename-extension": [
            0
        ],
        "object-curly-newline": [
            2,
            {
                "minProperties": 2,
                "consistent": true
            }
        ],
        "no-use-before-define": [
            "error",
            {
                "variables": false
            }
        ],
        "import/prefer-default-export": "off"
    }
}